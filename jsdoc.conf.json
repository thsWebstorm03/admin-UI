{
  "plugins": [
    "node_modules/jsdoc-babel"
  ],
  "babel": {
    "babelrc": false,
    "extensions": [
      "js"
    ],
    "presets": [
      [
        "env",
        {
          "targets": {
            "node": "6.11"
          }
        }
      ],
      "stage-2"
    ],
    "plugins": [
      "transform-flow-strip-types"
    ]
  },
  "opts": {
    "template": "node_modules/minami",
    "destination": "./docs/"
  }
}